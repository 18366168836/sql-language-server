{
  "name": "sql-language-server",
  "version": "0.0.1",
  "main": "./out/extension",
  "author": "joe-re <joe.tialtngo@gmail.com>",
  "license": "MIT",
  "publisher": "joe-re",
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "compile": "npm run compile:client && npm run compile:server",
    "compile:client": "tsc -p .",
    "compile:server": "tsc -p server",
    "watch": "run-p watch:client watch:server",
    "watch:client": "tsc -w -p .",
    "watch:server": "tsc -w -p server"
  },
  "engines": {
    "vscode": "^1.24.0"
  },
  "activationEvents": [
    "onLanguage:sql"
  ],
  "dependencies": {
    "vscode": "^1.1.18",
    "vscode-languageclient": "^4.2.1"
  },
  "devDependencies": {
    "@types/node": "^10.0.3",
    "ava": "1.0.0-beta.4",
    "npm-run-all": "^4.1.3",
    "parcel-bundler": "^1.7.1",
    "typescript": "^2.8.3"
  }
}
